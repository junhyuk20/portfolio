<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="my.web.note.dao.MemberMapper">
	
	<insert id="memberInsert" parameterType="member">
			insert into
				m_table(
					member_no
					,member_id
					,member_pw
					,member_nm
				)values(
					m_table_seq.nextval
					,#{member_id}	
					,#{member_pw}
					,#{member_nm}
				)
	</insert>
	
	<select id="memberSelectOne" parameterType="String" resultType="member">
		select
			member_no
			,member_id
			,member_pw
			,member_nm
		from
			m_table
		where
			member_id = #{member_id}
	</select>
	
	<update id="memberUpdate" parameterType="member">
		update
			m_table
		set
			member_id = #{member_id}
			,member_pw = #{member_pw}
			,member_nm = #{member_nm}
		where
			member_id = #{member_id}
	</update>
</mapper>
